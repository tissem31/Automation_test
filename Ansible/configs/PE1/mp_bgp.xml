            <bgp>
                <log-neighbor-changes>true</log-neighbor-changes>
            </bgp>
            <neighbor>
                <id>10.255.255.12</id>
                <remote-as>64512</remote-as>
                <update-source>
                    <Loopback>0</Loopback>
                </update-source>
            </neighbor>
            <neighbor>
                <id>10.255.255.13</id>
                <remote-as>64512</remote-as>
