            </neighbor>
            <address-family>
                <no-vrf>
                    <ipv4>
                        <af-name>unicast</af-name>
                        <ipv4-unicast>
                            <neighbor>
                                <id>10.255.255.12</id>
                                <activate />
                            </neighbor>
                            <neighbor>
                                <id>10.255.255.13</id>
                                <activate />
                            </neighbor>
                            <neighbor>
                                <id>10.255.255.14</id>
                                <activate />
                            </neighbor>
                        </ipv4-unicast>
                    </ipv4>
                    <vpnv4>
                        <af-name>unicast</af-name>
                        <vpnv4-unicast>
                            <neighbor>
                                <id>10.255.255.12</id>
                                <activate />
                                <send-community>
                                    <send-community-where>extended</send-community-where>
                                </send-community>
                            </neighbor>
                            <neighbor>
                                <id>10.255.255.13</id>
                                <activate />
                                <send-community>
                                    <send-community-where>extended</send-community-where>
                                </send-community>
                            </neighbor>
                            <neighbor>
                                <id>10.255.255.14</id>
                                <activate />
                                <send-community>
                                    <send-community-where>extended</send-community-where>
                                </send-community>
                            </neighbor>
                        </vpnv4-unicast>
                    </vpnv4>
                </no-vrf>
            </address-family>
        </bgp>
    </router>
  </native>
</config>
