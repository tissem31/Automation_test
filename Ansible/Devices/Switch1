config t 
interface Vlan100
 ip address 10.10.0.100 255.255.255.0
!
interface GigabitEthernet0/0
 no switchport
 ip address 172.20.0.108 255.255.255.0
 no negotiation auto
!
interface GigabitEthernet0/1
 switchport access vlan 100
 duplex full
 no negotiation auto
 !
 interface GigabitEthernet0/2
 switchport access vlan 100
 duplex full
 no negotiation auto
 !
 interface GigabitEthernet0/3
 switchport access vlan 100
 duplex full
 no negotiation auto
